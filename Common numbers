# given tw0 lists A, B, the function returns the common numbers.
def common_numbers(A,B):

  string = ''
  for a in (A if len(A) < len(B) else B):
    if a in B:
      string += str(a)
    else:
      continue

  lists = [0]*len(string)
  count = 0 
  for s in string:
    lists[count] = int(s)
    count += 1

  return lists

A = [1, 3, 4, 6, 7]
B = [1, 2, 4, 5, 9, 10, 6]
print(common_numbers(A,B))
